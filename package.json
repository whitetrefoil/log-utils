{
  "name": "@whitetrefoil/log-utils",
  "version": "0.4.1",
  "description": "Some browser console.log utilities inspired by debug",
  "keywords": [
    "log",
    "console",
    "debug"
  ],
  "files": [
    "lib/main.*",
    "src",
    "tests",
    "tsconfig*.json"
  ],
  "source": "src/main.ts",
  "main": "lib/main.js",
  "module": "lib/main.module.js",
  "unpkg": "lib/main.umd.js",
  "typings": "lib/main.d.ts",
  "scripts": {
    "prepublishOnly": "node scripts/prepublish-checklist.js && npm run build",
    "build": "rimraf lib && microbundle --raw --tsconfig src/tsconfig.json",
    "publish": "echo \"Done publishing, don't forget to:\n  1. **dist-tag** your channel in npm.\n  2. bump version in package.json\nGood luck with the publish ;)\"",
    "test": "eslint . && jest --no-cache",
    "watch": "microbundle watch --tsconfig src/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whitetrefoil/log-utils.git"
  },
  "author": "WhiteTrefoil <whitetrefoil@gmail.com>",
  "license": "Unlicense",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "clean-stack": "^3.0.1",
    "extract-stack": "^2.0.0",
    "slash": "^3.0.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@whitetrefoil/eslint-config": "^0.18.0",
    "eslint": "^7.17.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results/jest",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": false,
        "tsConfig": "tests/tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "preset": "ts-jest",
    "setupFiles": [],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "testMatch": null,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transform": {
      ".tsx?": "ts-jest"
    }
  }
}
